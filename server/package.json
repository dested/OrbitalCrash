{
  "name": "server",
  "version": "1.0.1",
  "description": "",
  "scripts": {
    "link-stuff": "mklink /H .\\schema.prisma ..\\api\\prisma\\schema.prisma && mklink /H .\\.env ..\\api\\.env && mklink /H .\\.env.dev ..\\api\\.env.dev && mklink /H .\\.env.prod ..\\api\\.env.prod",
    "build": "rimraf dist && webpack --mode development",
    "watch": "rimraf dist && webpack --mode development --watch",
    "codegen": "graphql-codegen --config codegen.yml",
    "pretty": "npx prettier \"*/**/*.ts*\"  --config .prettierrc --write",
    "lint": "npx tslint -p tsconfig.json --fix",
    "nodemon": "nodemon",
    "start": "node --expose-gc ./dist/index.js",
    "debug": "node --expose-gc --inspect-brk ./dist/index.js",
    "build-prod": "rimraf dist && webpack --mode production  --progress --hide-modules",
    "deploy": "yarn build-prod && eb deploy && node ../auto-scaler/dist/index.js post-deploy",
    "generate": "prisma generate",
    "postinstall": "npm run generate"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.3",
    "@prisma/cli": "^2.0.0-beta.3",
    "apollo-boost": "^0.4.7",
    "graphql": "^14.6.0",
    "dotenv": "^8.2.0",
    "ws": "^7.2.1",
    "jsonwebtoken": "^8.5.1",
    "collisions": "^2.0.13",
    "bufferutil": "^4.0.1",
    "node-fetch": "^2.6.0",
    "utf-8-validate": "^5.0.2",
    "redis": "^3.0.2",
    "moment": "^2.24.0",
    "quickselect": "^2.0.0",
    "aws-sdk": "^2.658.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.2",
    "@graphql-codegen/introspection": "1.13.2",
    "@graphql-codegen/schema-ast": "1.13.2",
    "@graphql-codegen/typescript": "1.13.2",
    "@graphql-codegen/typescript-graphql-files-modules": "1.13.2",
    "@graphql-codegen/typescript-operations": "1.13.2",
    "@graphql-codegen/typescript-document-nodes": "1.13.2",
    "@graphql-codegen/typescript-resolvers": "1.13.2",
    "@types/moment": "^2.13.0",
    "@types/graphql": "^14.5.0",
    "@types/node-fetch": "^2.5.6",
    "@types/node": "13.9.2",
    "@types/redis": "^2.8.17",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "webpack-node-externals": "^1.7.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.7.0",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "copy-webpack-plugin": "^5.1.1"
  }
}
